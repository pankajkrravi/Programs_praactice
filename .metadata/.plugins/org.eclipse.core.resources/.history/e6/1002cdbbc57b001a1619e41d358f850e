
public class Employee {
	
	int id;
	String name;
	public Employee( int id, String name) {
	  this.id=id;
	  this.name=name;	
	}
	
	double CalculateBonus(double salary)
	{
		return salary*.10/100;
		
	}

	@Override
	public String toString() {
		return "Employee [id=" + id + ", name=" + name + "]";
	}

}
